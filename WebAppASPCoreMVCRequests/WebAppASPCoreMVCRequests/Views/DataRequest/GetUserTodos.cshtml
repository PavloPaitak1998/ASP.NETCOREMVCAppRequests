@model IEnumerable<(int id,string name)>

@{
    ViewData["Title"] = "GetUserTodos";
}

<h2>
    List (id, name) from list todos which is complete for user
</h2>

<div class="data">
    <div class="container data">
        <table class="table table-bordered text-center">
            <thead class="thead-dark">
                <tr>
                    <th class="">Todo ID</th>
                    <th class="">Name</th>
                </tr>
            </thead>
            @foreach (var t in Model)
            {
                <tr>
                    <td>@t.id </td>
                    <td>
                        <a asp-controller="DataRequest" asp-action="TodoInfo" asp-route-id="@t.id">
                            @t.name
                        </a>
                    </td>
                </tr>
            }
        </table>
    </div>
</div>