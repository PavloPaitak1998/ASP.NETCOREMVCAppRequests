@model List<WebAppASPCoreMVCRequests.Models.User>
@{
    ViewData["Title"] = "GetAllData";
}
<div class="container data">
    <table class="table table-bordered text-center">
        <thead class="thead-dark">
            <tr>
                <th>User ID</th>
                <th>Name</th>
                <th>Avatar</th>
                <th>Email</th>
                <th>Created At</th>
                <th>Posts</th>
                <th>Todos</th>
            </tr>
        </thead>
        @foreach (var u in Model)
        {
            <tr>
                <td>@u.Id </td>
                <td>@u.Name </td>
                <td>@u.Avatar </td>
                <td>@u.Email </td>
                <td>@DateTime.Parse(u.CreatedAt).ToUniversalTime() </td>
                <td>Post</td>
                <td>Todo</td>
            </tr>
        }
    </table>
</div>

